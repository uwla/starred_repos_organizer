{
    "name": "git_stars_organizer",
    "private": false,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "build": "tsc && vite build",
        "build:demo": "NODE_ENV=demo npm run build",
        "dev": "vite",
        "dev:rest": "concurrently --kill-others \"npm run serve:*\"",
