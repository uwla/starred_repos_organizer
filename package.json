        "@mui/material": "^5.16.8",
        "axios": "^1.6.7",
        "bootstrap": "^5.3.2",
        "export-from-json": "^1.7.4",
        "json-server": "^1.0.0-alpha.23",
        "prettier": "^3.6.2",
        "react": "^18.2.0",
        "react-bootstrap": "^2.10.1",
        "react-dom": "^18.2.0",
        "react-select": "^5.7.7",
        "tsx": "^4.7.1",
        "use-file-picker": "^2.1.1"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
